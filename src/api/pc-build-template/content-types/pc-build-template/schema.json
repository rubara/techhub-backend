{
  "kind": "collectionType",
  "collectionName": "pc_build_templates",
  "info": {
    "singularName": "pc-build-template",
    "pluralName": "pc-build-templates",
    "displayName": "pc build template"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "nameBg": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "basePrice": {
      "type": "decimal",
      "required": true
    },
    "platform": {
      "type": "enumeration",
      "enum": [
        "AMD",
        "Intel"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text"
    },
    "descriptionBg": {
      "type": "text"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "build_component": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::build-component.build-component",
      "inversedBy": "pc_build_templates"
    }
  }
}
